{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "14.17.6"
  },
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --exit-child --respawn --transpile-only --ignore-watch node_modules src/index.ts ",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": " tsc --build tsconfig.json && yarn prisma:generate && yarn cpy && prisma:migrate && yarn node src/index.js",
    "prisma:generate": "prisma generate --schema prisma/schemaMongo.prisma && prisma generate --schema prisma/schemaPostgres.prisma",
    "prisma:migrate": "prisma migrate deploy --schema=prisma/schemaPostgres.prisma",
    "cpy": "cp -R ./prisma ./dist && cp ./src/graphQL/schema.graphql ./dist/graphQL && cp ./package.json ./dist && cp ./vercel.json ./dist",
    "postgres:start": "docker-compose up -d",
    "vercel:build": "npm run prisma:generate && npm run prisma:migrate && tsc --build tsconfig.json && npm run cpy",
    "start": "node index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@graphql-tools/schema": "^8.3.13",
    "@prisma/client": "^3.14.0",
    "@types/node": "^17.0.36",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "express-graphql": "^0.12.0",
    "graphql": "^16.5.0",
    "graphql-import": "^1.0.2",
    "graphql-tools": "^8.2.11",
    "jsonwebtoken": "^8.5.1",
    "prisma": "^3.14.0",
    "superstruct": "^0.15.4",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.16.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-import-helpers": "^1.2.1",
    "prettier": "^2.6.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0"
  }
}
